# ========================================
# Private Key Configuration
# ========================================
# Your deployment wallet private key (WITHOUT 0x prefix)
PRIVATE_KEY=your_private_key_here

# ========================================
# RPC URLs
# ========================================
# Sepolia (Oracle chain)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Arbitrum Sepolia (Consumer chain)
ARBITRUM_SEPOLIA_RPC_URL=https://arb-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Base Sepolia (Consumer chain)
BASE_SEPOLIA_RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# ========================================
# Etherscan API Keys (for contract verification)
# ========================================
ETHERSCAN_API_KEY=your_etherscan_api_key_here
ARBISCAN_API_KEY=your_arbiscan_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# ========================================
# Deployed Contract Addresses
# ========================================
# Fill these after deployment
ORACLE_ADDRESS_SEPOLIA=
CACHE_ADDRESS_ARBITRUM=
CACHE_ADDRESS_BASE=

# ========================================
# Optional: Custom Gas Settings
# ========================================
# Uncomment if you want to set custom gas parameters
# GAS_PRICE=20000000000
# GAS_LIMIT=5000000
